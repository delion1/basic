  "description": "basic project github.io",
  "main": "index.js",
  "dependencies": {
    "bourbon-neat": "^2.1.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^4.0.1",
    "gulp-shell": "^0.6.5",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^5.0.0",
    "node-bourbon": "^4.2.8",
    "node-neat": "^2.0.0-beta.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "npm-run-notify": "^0.1.0",
    "onchange": "^6.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "uglify-js": "^3.6.0"
  },
  "devDependencies": {
    "gulp": "^4.0.0",
    "node-bourbon": "^4.2.8"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "notify": "npm-run-notify --message 'Compile Complete' --title 'SASS'",
    "scss": "node-sass --output-style compact -o css sass -- yarn run notify",
    "uglify": "uglifyjs js/scripts.js -b -m -o js-min/scripts-min.js",
    "watch:css": "onchange 'sass/*.scss' -- yarn run scss",
    "watch:js": "onchange 'js/*.js' -- yarn run uglify",
    "watch": "npm-run-all --parallel watch:css watch:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delion1/basic.git"
  },
